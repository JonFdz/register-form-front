{
    "version": 2,
    "builds": [
        {
            "src": "src/environments/environment.prod.ts",
            "use": "@vercel/static-build",
            "config": {
                "env": {
                    "API_URL_ACTIVITIES": "@api-url-activities",
                    "API_URL_INSCRIPTIONS": "@api-url-inscriptions",
                    "API_URL_USERS": "@api-url-users",
                    "API_URL_AUTH": "@api-url-auth"
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
